ck>{{''+good.goodsSpecificationName+''}}</view><view>{{"￥"+good.goodsReceiptsPrice}}</view></view></view><view class="compleEvaluated"><view class="completeNum">{{"X"+good.goodsNum}}</view><block wx:if="{{item.orderStatus==4&&good.evaluateStatus==0}}"><view data-event-opts="{{[['tap',[['toEvaluated',['$0','$1'],[[['olderData','',index]],[['olderData','',index],['orderGoodsList','',index]]]]]]]}}" class="Evaluated" bindtap="__e">去评价</view></block></view></view></view></block><view class="totalItem"><view class="totalContact"><view class="totalPay"><view class="piece">{{"共"+item.totalNum+"件，实付"}}</view><view class="pieces"><text style="font-size:22rpx;color:red;">￥</text>{{item.officialReceiptsPrice}}</view></view></view><view class="contactItem"><button class="contact" open-type="contact"><view style="line-height:60rpx;">联系客服</view></button><block wx:if="{{item.orderStatus==0||item.orderStatus==2||item.orderStatus==3||item.orderStatus==4&&item.orderStatus==0}}"><view data-event-opts="{{[['tap',[['getOrder',['$0'],[[['olderData','',index]]]]]]]}}" class="anothe" bindtap="__e">再来一单</view></block><block wx:if="{{item.orderStatus==1}}"><view class="anothe">立即支付</view></block></view></view></view></block></view>