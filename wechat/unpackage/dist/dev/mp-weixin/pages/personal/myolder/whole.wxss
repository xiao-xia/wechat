.noOrderItem {
  position: absolute;
  top: 50%;
  width: 100%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  text-align: center;
}
.noOrderItem .noOrderImg {
  width: 300rpx;
}
.noOrderItem .noOrderText {
  font-size: 25rpx;
  color: #aaa;
}
.noOrderItem .buttonColor {
  height: 60rpx;
  width: 190rpx;
  line-height: 60rpx;
  font-size: 13px;
  text-align: center;
  background-color: #008CFF;
  border-radius: 30rpx;
  color: #fff;
  margin-top: 20rpx;
}
.orderback {
  width: 100%;
  background-color: #fff;
  padding: 0 0 0 20rpx;
  margin-top: 20rpx;
}
.orderback .orderBox {
  display: flex;
  width: 100%;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  padding: 0 20rpx 0 10rpx;
  border-bottom: 1px  solid #f2f2f2;
}
.orderback .orderBox .orderfinsh {
  display: flex;
  align-items: center;
  justify-content: center;
}
.orderback .orderBox .orderfinsh .finshWord {
  font-size: 30rpx;
  color: #FE771E;
}
.orderback .orderBox .orderfinsh .finshTime {
  margin-left: 20rpx;
  font-size: 24rpx;
  color: #ccc;
}
.orderback .orderBox .right {
  color: #ccc;
}
.orderback .Completed {
  width: 100%;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  justify-content: space-between;
  padding: 30rpx 20rpx 30rpx 10rpx;
}
.orderback .Completed .completeLeft {
  display: flex;
  align-items: center;
}
.orderback .Completed .completeLeft .CompletedImg {
  width: 120rpx;
  height: 120rpx;
}
.orderback .Completed .completeLeft .CompletedImg image {
  width: 100%;
  height: 100%;
}
.orderback .Completed .completeLeft .completedMove {
  display: flex;
}
.orderback .Completed .completeLeft .completedMove .completePrice {
  margin-left: 30rpx;
  width: 420rpx;
}
.orderback .Completed .completeLeft .compleEvaluated {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.orderback .Completed .completeLeft .compleEvaluated .completeNum {
  color: #333333;
  font-size: 24rpx;
}
.orderback .Completed .completeLeft .compleEvaluated .Evaluated {
  color: #FE771E;
  border: 1px solid #FE771E;
  width: 120rpx;
  height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  border-radius: 30rpx;
  line-height: 45rpx;
  margin-top: 10rpx;
}
.orderback .totalItem {
  width: 100%;
  padding: 30rpx 20rpx 30rpx 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.orderback .totalItem .totalContact {
  display: flex;
}
.orderback .totalItem .totalContact .totalPay {
  display: flex;
  align-items: center;
}
.orderback .totalItem .totalContact .totalPay .piece {
  color: #333333;
  font-size: 24rpx;
}
.orderback .totalItem .totalContact .totalPay .pieces {
  font-size: 32rpx;
  color: red;
}
.orderback .totalItem .contactItem {
  display: flex;
}
.orderback .totalItem .contactItem .contact {
  height: 60rpx;
  color: #CCCCCC;
  background-color: #FFFFFF;
  border-radius: 30rpx;
  border: 1px solid #ccc;
  text-align: center;
  font-size: 26rpx;
  margin-right: 20rpx;
}
.orderback .totalItem .contactItem .anothe {
  width: 150rpx;
  height: 60rpx;
  color: #FFFFFF;
  background-color: #FE771E;
  line-height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  font-size: 26rpx;
}
.orderback .totalItem .contactItem .seckanothe {
  width: 150rpx;
  height: 60rpx;
  color: #FFFFFF;
  background-color: #ccc;
  line-height: 60rpx;
  border-radius: 30rpx;
  text-align: center;
  font-size: 26rpx;
}
.bar {
  width: 100%;
  height: 100rpx;
  margin: 0 0 20rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 66rpx;
  background-color: #FFFFFF;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 9999;
}
.bar .barBlockStyle {
  font-size: 28rpx;
  color: #008CFF;
  line-height: 100rpx;
  text-align: center;
}
.bar .barBlock {
  font-size: 28rpx;
  color: #101010;
  line-height: 100rpx;
  text-align: center;
}
.card {
  width: 100%;
  margin-bottom: 20rpx;
  padding: 20rpx 40rpx;
  background-color: #fff;
}
.card .top {
  display: flex;
  justify-content: space-between;
}
.card .num {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.card .num .photo {
  display: flex;
  align-items: center;
}
.card .num .photo .wrap {
  width: 200rpx;
  height: 200rpx;
}
.card .num .photo .wrap image {
  width: 100%;
  height: 100%;
}
.card .choose {
  display: flex;
  justify-content: flex-end;
  margin-top: 20rpx;
}
.card .choose .lbtn {
  width: 200rpx;
  line-height: 60rpx;
  height: 60rpx;
  text-align: center;
  border: 1px solid #eee;
  color: #aaa;
  margin-right: 20rpx;
}
.card .choose .rbtn {
  width: 200rpx;
  line-height: 60rpx;
  height: 60rpx;
  text-align: center;
  color: var(--button);
  border: 1px solid var(--button);
}

