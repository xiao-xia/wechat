page {
  width: 100%;
  height: 100%;
}
.selectStatus {
  color: #1395f4;
  border-bottom: 4rpx solid #1395f4;
}
.collectWrapper {
  width: 100%;
  height: 100%;
  z-index: -10;
}
.collectWrapper .agentItem {
  width: 100%;
  height: 660rpx;
  width: 100%;
  height: 658rpx;
  position: absolute;
}
.collectWrapper .agentItem image {
  width: 100%;
  height: 100%;
  z-index: -5;
}
.collectWrapper .agentItem .users {
  display: flex;
  width: 100%;
  position: absolute;
  top: 0;
  z-index: 999;
}
.collectWrapper .agentItem .users .back {
  color: #fff;
  padding-left: 10rpx;
}
.collectWrapper .agentItem .users .user {
  color: #fff;
  width: 100%;
  text-align: center;
}
.collectWrapper .uni-padding-wrap {
  height: 100%;
}
.collectWrapper .uni-padding-wrap .page-section-spacing {
  height: 100%;
}
.collectWrapper .uni-padding-wrap .swiper {
  height: 100%;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox {
  width: 100%;
  height: auto;
  display: flex;
  justify-content: center;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .logoColor {
  position: absolute;
  font-size: 20px;
  top: 190rpx;
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  background-color: #fff;
  color: #333333;
  border-radius: 50%;
  text-align: center;
  font-weight: bold;
  box-shadow: 0 0 5px 1px rgba(0, 0, 0, 0.1);
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack {
  width: 94%;
  background-color: #FFFFFF;
  margin-top: 130px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove {
  padding: 20rpx;
  height: 100vh;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .agentName {
  text-align: center;
  margin-top: 80rpx;
  color: #333333;
  font-size: 32rpx;
  font-weight: 550;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .Subtitle {
  text-align: center;
  color: #666666;
  margin-top: 20rpx;
  font-size: 30rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .keynote {
  color: #F77B44;
  margin: 20rpx 0 20rpx 0;
  font-size: 28rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .Basic {
  color: #666666;
  font-size: 28rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .consulting {
  width: 200rpx;
  height: 80rpx;
  background-color: #50ADF9;
  color: #FFFFFF;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  border-radius: 40rpx;
  margin: 50rpx 180rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .mailbox {
  color: #9B9B9B;
  font-size: 26rpx;
  margin-left: 20rpx;
}
.collectWrapper .uni-padding-wrap .swiper .logoBox .writeBack .writeMove .address {
  color: #9B9B9B;
  font-size: 26rpx;
  margin-left: 20rpx;
}

