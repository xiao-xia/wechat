.container {
  width: 100%;
  height: 100%;
  position: relative;
}
.users {
  display: flex;
  width: 100%;
  position: absolute;
  top: 0;
}
.users .back {
  color: #fff;
  padding-left: 10rpx;
}
.users .user {
  color: #fff;
  padding-left: 10rpx;
  width: 100%;
  text-align: center;
}
.userInfoItem {
  width: 100%;
  height: 470rpx;
}
.userInfoItem .main {
  position: absolute;
  top: 180rpx;
  left: 0;
  width: 100%;
  padding: 0 20rpx;
}
.userInfoItem image {
  width: 100%;
  height: 100%;
}
.userInfoItem .userInfo {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.userInfoItem .userInfo .userAvatar {
  width: 120rpx;
  height: 120rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.userInfoItem .userInfo .userAvatar .userAvatarimg {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.userInfoItem .userInfo .userInfoTexts {
  align-items: center;
  justify-content: space-between;
  width: calc(100% - 150rpx);
}
.userInfoItem .userInfo .userInfoTexts .userInfoVipbar {
  display: flex;
  align-items: center;
}
.userInfoItem .userInfo .userInfoTexts .identity {
  width: 150rpx;
  height: 50rpx;
  background-color: #0048c3;
  border-radius: 30rpx;
  color: #fff;
  text-align: center;
}
.userInfoItem .userInfo .userInfoText {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: calc(100% - 150rpx);
}
.orderItem {
  width: 100%;
  -webkit-transform: translateY(-50rpx);
          transform: translateY(-50rpx);
}
.orderItem .orderbox {
  width: 100%;
  height: 100%;
  flex-shrink: 0;
  text-align: center;
  font-size: 28rpx;
  background-size: 100% 100%;
  margin-top: -70rpx;
}
.orderItem .orderbox .menberItem {
  z-index: 2;
  display: flex;
  margin-top: -300rpx;
  margin-left: 130rpx;
}
.orderItem .orderbox .menberItem .menberege {
  margin-top: -20rpx;
}
.orderItem .orderbox .menberItem .menberege .memberImg {
  width: 130rpx;
  height: 40rpx;
}
.orderItem .orderbox .menberItem .menberege .memberImg image {
  width: 100%;
  height: 100%;
}
.orderItem .orderbox .menberItem .menberege .privilege {
  color: #c69b31;
  margin-top: 20rpx;
}
.orderItem .orderbox .menberItem .shipCard {
  border-radius: 10rpx;
  color: #ddaa2b;
  background-color: #ffff;
  text-align: center;
  height: 50rpx;
  width: 170rpx;
  line-height: 50rpx;
  font-size: 13px;
  margin-left: 140rpx;
}
.orderItem .orderbox .orderPersonal {
  margin: 20rpx 0 0rpx 0;
}
.orderItem .orderbox .orderPersonal .orderTitle {
  margin-left: 20rpx;
  margin-top: 10rpx;
}
.orderItem .orderbox .orderPersonal .orderTitle .orderTitleimg .shu {
  color: var(--button);
}
.orderItem .orderbox .orderPersonal .orderState {
  display: flex;
  padding: 30rpx 20rpx;
  align-items: center;
}
.orderItem .orderbox .orderPersonal .orderState .orderBlock {
  flex: 1;
  display: flex;
  justify-content: center;
  position: relative;
}
.orderItem .orderbox .orderPersonal .orderState .orderBlock .orderNumber {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background-color: orange;
  font-size: 20rpx;
  color: #fff;
  line-height: 30rpx;
  text-align: center;
  top: -15rpx;
  right: 50rpx;
}
.MyFunction {
  width: 100%;
  background-color: #ffffff;
  margin-top: -30rpx;
  padding: 20rpx;
}
.MyFunction .mytion {
  padding: 20rpx 0;
  font-weight: bold;
  font-size: 14px;
  color: #333333;
  margin: 10rpx 10rpx 10rpx 36rpx;
}
.MyFunction .aboutFunction {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}
.MyFunction .aboutFunction .mytionBox {
  width: 25%;
  text-align: center;
  margin-bottom: 20rpx;
}
.button {
  width: 100%;
  height: 80rpx;
  background-color: #008cff;
  color: white;
  border-radius: 0;
  line-height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 0;
  font-size: 30rpx;
}
.functionBlcok {
  width: 100%;
  height: 100rpx;
  padding: 0 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #f0f0f0;
}
.functionBlcok .functionLeft {
  display: flex;
  align-items: center;
}
.functionBlcok .functionLeft .functionText {
  font-size: 28rpx;
  margin-left: 20rpx;
  color: #666666;
}
.functionBlcok .functionLeft .functionIcon {
  font-size: 40rpx;
}
.functionBlcok .functionLeft .functionImg {
  width: 50rpx;
  height: 50rpx;
}
.functionBlcok:last-child {
  border: none;
}
.editionItem {
  font-size: 28rpx;
  color: #a4a5a5;
  text-align: center;
  line-height: 70rpx;
}
.camera {
  width: 180rpx;
  height: 180rpx;
  background-color: #fff;
  margin: 0 auto;
}
.padding-xl {
  display: flex;
  justify-content: center;
}
.conform {
  width: 180rpx;
  height: 60rpx;
  background-color: var(--button);
  font-size: 30rpx;
  color: #fff;
  line-height: 60rpx;
  border-radius: 10rpx;
}
.swiper-item {
  width: 100%;
  height: 700rpx !important;
  display: flex;
  align-items: center;
  justify-content: center;
}
.swiper-item .checkImg {
  width: 100%;
}
.noUserAvatarimg {
  width: 70rpx !important;
  height: 70rpx !important;
}
.upImg {
  width: 100%;
  height: 100%;
}
.cu-dialog {
  width: 500rpx;
  background-color: #fff;
}
.cu-bar {
  height: 50rpx !important;
