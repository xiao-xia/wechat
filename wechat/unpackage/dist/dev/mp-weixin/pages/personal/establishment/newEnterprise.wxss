.container .establishback {
  width: 100%;
  height: 1200rpx;
  background-color: #fff;
  margin-top: 10rpx;
}
.container .establishback .estable {
  padding: 10rpx 0 10rpx 30rpx;
}
.container .establishback .estable .establishName {
  font-size: 14px;
  color: #333333;
}
.container .establishback .estable .inputClass {
  background-color: #F1F1F1;
  width: 94%;
  height: 72rpx;
  padding: 5rpx 8rpx 0 2rpx;
}
.container .establishback .estable .inputClass .inputFrame {
  width: 100%;
  height: 60rpx;
  font-size: 14px;
  padding: 0 20rpx;
}
.container .establishback .estableTab {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.container .establishback .estableTab .services {
  width: 130rpx;
  height: 50rpx;
  background-color: #F1F1F1;
  border-radius: 10rpx;
  font-size: 28rpx;
  text-align: center;
  color: #CCCCCC;
  line-height: -40rpx;
  margin: 20rpx 40rpx 0 0;
}
.container .establishback .estableTab .active {
  width: 130rpx;
  height: 50rpx;
  background-color: #008CFF;
  border-radius: 10rpx;
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  line-height: -40rpx;
  margin: 20rpx 40rpx 0 0;
}
.container .establishback .buttonadd {
  background-image: linear-gradient(to right, #1093FE, #55AFF9);
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  width: 500rpx;
  margin: 0  auto;
  color: #fff;
  border-radius: 50rpx;
  font-size: 32rpx;
  margin-top: 20rpx;
}

