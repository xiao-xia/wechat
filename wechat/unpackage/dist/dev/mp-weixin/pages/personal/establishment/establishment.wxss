.container {
  height: auto;
}
.container .improvement {
  width: 100%;
  background-color: #B1D9FB;
  display: flex;
  align-items: center;
}
.container .companyBack {
  background-color: #fff;
  margin: 20rpx;
}
.container .companyBack .companyBox {
  margin: 20rpx;
  width: 100%;
}
.container .companyBack .companyBox .companyMove {
  display: flex;
  align-items: center;
}
.container .companyBack .companyBox .companyMove .companySize {
  font-size: 30rpx;
}
.container .Notyet {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  height: 800rpx;
  justify-content: center;
}
.container .Notyet .estmentimg {
  width: 400rpx;
  height: 400rpx;
}
.container .Notyet .estmentimg image {
  width: 100%;
  height: 100%;
}
.container .Notyet .notext {
  color: #999999;
  font-size: 32rpx;
}
.container .noestabItem {
  position: absolute;
  top: 40%;
  width: 100%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  text-align: center;
}
.container .noestabItem .noestabImg {
  width: 300rpx;
}
.container .noestabItem .noestabText {
  font-size: 28rpx;
  color: #aaa;
}
.container .buttonBack {
  width: 100%;
  background-color: #FFFFFF;
  position: fixed;
  left: 0;
  bottom: 0;
  height: 140rpx;
  display: flex;
  justify-content: center;
}
.container .buttonBack .buttonSpan {
  width: 80%;
  margin-top: 20rpx;
}
.container .buttonBack .buttonSpan .buttonColor {
  width: 92%;
  height: 100rpx;
  background-image: linear-gradient(to right, #1093FE, #55AFF9);
  color: white;
  line-height: 100rpx;
  border-radius: 50rpx;
}

