  padding: 20rpx;
  display: flex;
  justify-content: space-between;
}
.orderset .setproduct .amount .amountbot text {
  font-size: 30rpx;
}
.orderset .setproduct .pieces .piecepro {
  padding: 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #eee;
}
.orderset .setproduct .pieces .piecepro text {
  font-size: 30rpx;
}
.orderset .setproduct .setdistribution {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #eee;
  padding: 0 20rpx;
}
.orderset .setproduct .total {
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  font-size: 30rpx;
}
.beizhu {
  display: flex;
  justify-content: space-between;
  padding-left: 20rpx;
  background-color: #fff;
  border-radius: 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
}
.beizhu .right {
  display: flex;
  align-items: center;
}
.setbottom {
  margin-top: 20rpx;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #e4e4e4;
}
.setbottom .setends {
  padding-left: 30rpx;
  display: flex;
  justify-content: space-between;
  background-color: #FFFFFF;
}
.setbottom .setends .endscont {
  font-size: 30rpx;
}
.setbottom .setends .endscont .discount {
  color: #9b9b9b;
}
.setbottom .setends .endscont text {
  display: block;
}
.setbottom .setends .endbutton {
  height: 100rpx;
  line-height: 100rpx;
  width: 180rpx;
  color: #fff;
  background-color: #F86500;
  text-align: center;
}
.addHome {
  width: 230rpx;
  height: 80rpx;
  border: 2rpx solid #04C891;
  color: #04C891;
