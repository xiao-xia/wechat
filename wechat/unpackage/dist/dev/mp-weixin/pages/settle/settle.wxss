page {
  height: 100%;
}
.orderset {
  width: 100%;
  padding: 20rpx;
  height: 100%;
  position: relative;
}
.orderset .coupon {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}
.orderset .coupon .couponlist {
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  background-color: #F6F6F6;
}
.orderset .coupon .couponlist .top {
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  background-color: #fff;
  padding: 20rpx;
  position: relative;
}
.orderset .coupon .couponlist .top .guanbi {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
}
.orderset .coupon .couponlist .top .down {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.orderset .coupon .couponlist .showList {
  padding: 30rpx;
}
.orderset .coupon .couponlist .showList .card {
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  display: flex;
  padding: 20rpx;
  align-items: center;
  border: 1px solid #eee;
}
.orderset .coupon .couponlist .showList .rule {
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  padding: 0 20rpx;
}
.orderset .myhome, .orderset .setproduct {
  background-color: #fff;
  border-radius: 20rpx;
}
.orderset .myhome .homecont, .orderset .setproduct .homecont {
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
}
.orderset .myhome .homecont .cuIcon-right, .orderset .setproduct .homecont .cuIcon-right {
  align-self: center;
}
.orderset .myhome .homecont .homebor, .orderset .setproduct .homecont .homebor {
  border-radius: 20rpx;
  background-color: #f2f2f2;
  color: #b4b4b4;
  width: 150rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
}
.orderset .myhome .homecont .bottomBlock, .orderset .setproduct .homecont .bottomBlock {
  display: flex;
  align-self: center;
  margin-left: 20rpx;
}
.orderset .myhome .homecont .homeadd, .orderset .setproduct .homecont .homeadd {
  margin-left: 30rpx;
}
.orderset .myhome .settime, .orderset .setproduct .settime {
  padding: 10rpx 30rpx;
}
.orderset .myhome .settime, .orderset .myhome .setright, .orderset .setproduct .settime, .orderset .setproduct .setright {
  display: flex;
  justify-content: space-between;
}
.orderset .myhome .settime text, .orderset .myhome .settime .cuIcon-right, .orderset .myhome .setright text, .orderset .myhome .setright .cuIcon-right, .orderset .setproduct .settime text, .orderset .setproduct .settime .cuIcon-right, .orderset .setproduct .setright text, .orderset .setproduct .setright .cuIcon-right {
  height: 104rpx;
  line-height: 104rpx;
}
.orderset .myhome .settime .setpro, .orderset .myhome .setright .setpro, .orderset .setproduct .settime .setpro, .orderset .setproduct .setright .setpro {
  font-size: 36rpx;
  font-weight: bold;
}
.orderset .myhome .settime .setright .setdate, .orderset .myhome .setright .setright .setdate, .orderset .setproduct .settime .setright .setdate, .orderset .setproduct .setright .setright .setdate {
  margin-right: 20rpx;
  color: #ccc;
}
.orderset .setproduct {
  margin-top: 40rpx;
}
.orderset .setproduct .endproduct {
  margin-bottom: 20rpx;
  padding: 40rpx 0 0;
}
.orderset .setproduct .endproduct .head {
  padding-left: 20rpx;
  padding-right: 40rpx;
  display: flex;
  justify-content: space-between;
}
.orderset .setproduct .endproduct .proimg {
  padding: 0 20rpx;
  margin-top: 20rpx;
}
.orderset .setproduct .endproduct .proimg .producttycont {
  display: flex;
}
.orderset .setproduct .endproduct .proimg .producttycont .detail {
  margin-left: 40rpx;
}
.orderset .setproduct .endproduct .proimg .producttycont .producttys {
  width: 100rpx;
  height: 100rpx;
}
.orderset .setproduct .endproduct .proimg .producttycont .producttys image {
  width: 100%;
  height: 100%;
}
.orderset .setproduct .endproduct .proimg, .orderset .setproduct .endproduct .piecenum {
  display: flex;
  justify-content: space-between;
}
.orderset .setproduct .endproduct .piecenum text, .orderset .setproduct .endproduct .cuIcon-right text {
