  padding: 20rpx;
  border-radius: 10rpx;
}
.container .memberBack .openCard .buttonItem {
  width: 100%;
  position: absolute;
  top: 1070rpx;
  display: flex;
  justify-content: center;
}
.container .memberBack .openCard .buttonItem .buttonColor {
  width: 600rpx;
  height: 100rpx;
  text-align: center;
  background-image: linear-gradient(to right, #F8E6D7, #E7B596);
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .memberBack .openCard .buttonItem .buttonColor .limit {
  width: 60rpx;
  height: 30rpx;
