page {
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
}
.container .memberBack {
  position: relative;
  width: 100%;
  height: 100%;
}
.container .memberBack image {
  width: 100%;
  height: 100%;
}
.container .memberBack .openCard {
  width: 100%;
  z-index: 1;
  display: flex;
  justify-content: center;
}
.container .memberBack .openCard .openMember {
  color: #DEAA8A;
  font-size: 60rpx;
  position: absolute;
  top: 40rpx;
}
.container .memberBack .openCard .openMove {
  width: 100%;
  color: #DEAA8A;
  font-size: 60rpx;
  position: absolute;
  top: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}
.container .memberBack .openCard .colorItem {
  width: 100%;
  font-size: 60rpx;
  position: absolute;
  top: 120rpx;
  margin-top: 120rpx;
  text-align: center;
}
.container .memberBack .openCard .colorItem .colorWidth {
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .memberBack .openCard .colorItem .colorWidth .scroll-view_H {
  /* 文本不会换行，文本会在在同一行上继续，直到遇到 <br> 标签为止。 */
  white-space: nowrap;
  width: 100%;
}
.container .memberBack .openCard .colorItem .colorWidth .scroll-view-item_H {
  display: inline-block;
  width: 35%;
  height: 300rpx;
  border: 1rpx solid;
  text-align: center;
  font-size: 36rpx;
  margin: 2%  1% 2% 1%;
}
.container .memberBack .openCard .colorItem .colorWidth .scroll-view-item_H view {
  box-sizing: border-box;
}
.container .memberBack .openCard .colorItem .colorWidth .colorBack {
  width: 236rpx;
  background-image: linear-gradient(to bottom, #EED6A5, #E0BD7D);
  background-repeat: no-repeat;
  border-radius: 20rpx;
  padding: 30rpx;
  position: relative;
  overflow: hidden;
}
.container .memberBack .openCard .colorItem .colorWidth .colorBack .colorLittle {
  background-color: #BB865B;
  width: 70rpx;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  position: absolute;
  top: -1rpx;
  left: -1rpx;
  color: #fff;
  z-index: 999;
  border-radius: 0 0 10rpx 10rpx;
}
.container .memberBack .openCard .colorItem .colorWidth .colorBack .vIPword {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: relative;
}
.container .memberBack .openCard .colorItem .colorWidth .colorBack .selectItem {
  width: 100rpx;
  height: 100rpx;
  background-color: #A37550;
  position: absolute;
  right: -50rpx;
  bottom: -50rpx;
  -webkit-transform: rotate(225deg);
          transform: rotate(225deg);
}
.container .memberBack .openCard .colorItem .colorWidth .colorBack .selectItem .selectItemIcon {
  width: 100rpx;
  height: 100rpx;
  -webkit-transform: rotate(125deg);
          transform: rotate(125deg);
  color: #fff;
  font-size: 35rpx;
  line-height: 5rpx;
  margin-left: -7rpx;
  margin-top: -30rpx;
}
.container .memberBack .openCard .colorItem .colorWidth .writeBack {
  width: 236rpx;
  background-color: #FFFFFF;
  border-radius: 20rpx;
  padding: 30rpx;
  position: relative;
  overflow: hidden;
}
.container .memberBack .openCard .colorItem .colorWidth .writeBack .colorLittle {
  background-color: #BB865B;
  width: 70rpx;
  text-align: center;
  height: 40rpx;
  line-height: 40rpx;
  position: absolute;
  top: -1rpx;
  left: -1rpx;
  color: #fff;
  border-radius: 0 0 10rpx 10rpx;
}
.container .memberBack .openCard .colorItem .colorWidth .writeBack .vIPword {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  position: relative;
  z-index: 999;
}
.container .memberBack .openCard .privilegesItem {
  width: 100%;
  position: absolute;
  top: 550rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.container .memberBack .openCard .privilegesItem .privileges {
  color: #DEAA8A;
  font-size: 40rpx;
}
.container .memberBack .openCard .privilegesItem .privileges .line {
  display: inline-block;
  width: 70px;
  border: 1px dashed #cccccc;
  vertical-align: 5px;
  height: 4rpx;
  margin: 0 10px 0 10px;
}
.container .memberBack .openCard .privilegesItem .Equityvalue {
  font-size: 26rpx;
  background-color: #E7B394;
  color: #32303A;
