  height: 40rpx;
  line-height: 40rpx;
  border-radius: 40rpx;
}
.container .memberBack .openCard .specialItem {
  width: 100%;
  position: absolute;
  top: 700rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .memberBack .openCard .specialItem .Seckill {
  width: 180rpx;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.container .memberBack .openCard .specialItem .Seckill .area {
  color: #BC875D;
  font-size: 30rpx;
}
.container .memberBack .openCard .areaItem {
  width: 100%;
  position: absolute;
  top: 890rpx;
  display: flex;
  justify-content: center;
}
.container .memberBack .openCard .areaItem .areaBox {
  width: 660rpx;
  height: 140rpx;
  border: 1px solid #BC875D;
  display: flex;
