.confirmItem {
  padding: 20rpx;
  width: 100%;
  height: 100%;
}
.confirmItem .myhome {
  background-color: #fff;
  border-radius: 20rpx;
}
.confirmItem .myhome .homecont {
  padding: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.confirmItem .myhome .homecont .homeaddress {
  display: flex;
  align-items: center;
}
.confirmItem .myhome .homecont .homeaddress .img {
  width: 50rpx;
  height: 50rpx;
}
.confirmItem .myhome .homecont .homeaddress .add {
  margin-left: 20rpx;
}
.confirmItem .myhome .homecont .homeaddress .add .down {
  color: #ccc;
}
.confirmItem .ordercomfirm {
  background-color: #fff;
  border-radius: 20rpx;
}
.confirmItem .ordercomfirm .ordercont {
  padding: 20rpx;
}
.confirmItem .ordercomfirm .ordercont .message {
  font-size: 28rpx;
  border-bottom: 2rpx solid #eee;
  padding-bottom: 20rpx;
}
.confirmItem .ordercomfirm .ordercont .agreeyear {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.confirmItem .ordercomfirm .ordercont .agreeyear .vipradio {
  display: flex;
}
.confirmItem .ordercomfirm .ordercont .agreeyear checkbox-group {
  width: 60rpx;
  border-radius: 100%;
}
.confirmItem .ordercomfirm .ordercont .agreeyear checkbox .wx-checkbox-input {
  border-radius: 50% !important;
  color: #ffffff !important;
}
.confirmItem .ordercomfirm .ordercont .vipyear {
  display: flex;
  align-items: center;
  border-bottom: 2rpx solid #eee;
  justify-content: space-between;
  padding: 20rpx 20rpx 20rpx 0;
}
.confirmItem .ordercomfirm .ordercont .vipyear .vipradio {
  display: flex;
}
.confirmItem .ordercomfirm .ordercont .vipyear .radioIcon {
  width: 50rpx;
  height: 50rpx;
}
.confirmItem .ordercomfirm .ordercont .vipyear .radioName {
  font-size: 28rpx;
  color: #101010;
  margin-left: 20rpx;
}
.confirmItem .ordercomfirm .ordercont .vipyear .yearsize {
  font-size: 28rpx;
}
.confirmItem .ordercomfirm .ordercont .vipyear .price {
  font-size: 26rpx;
}
.confirmItem .ordercomfirm .ordercont .vipyear checkbox-group {
  width: 60rpx;
  border-radius: 100%;
}
.confirmItem .ordercomfirm .ordercont .vipyear checkbox .wx-checkbox-input {
  border-radius: 50% !important;
  color: #ffffff !important;
}
.confirmItem .ordercomfirm .ordercont .vipyear .redprice {
  font-size: 36rpx;
  color: red;
}
.confirmItem .setbottom {
  margin-top: 20rpx;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  background-color: #e4e4e4;
  border-top: 1rpx solid #f2f2f2;
  z-index: 999;
}
.confirmItem .setbottom .setends {
  padding-left: 30rpx;
  display: flex;
  justify-content: space-between;
  background-color: #FFFFFF;
}
.confirmItem .setbottom .setends .endscont .discount {
  color: #9b9b9b;
}
.confirmItem .setbottom .setends .endbutton {
  height: 110rpx;
  line-height: 110rpx;
  width: 180rpx;
  color: #fff;
  background-color: #F86500;
  text-align: center;
}

