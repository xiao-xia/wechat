<view class="location"><view class="place"><text class="cuIcon-locationfill lg" style="margin-right:20rpx;color:#00ad87;"></text><block wx:if="{{addressObj}}"><text>{{"默认送至:"+(addressObj.receiverCell||'')}}</text></block><block wx:else><text>请添加收货地址</text></block></view><view class="current"><view style="color:#aaa;">当前定位</view><view>{{''+msg+''}}</view></view><map latitude="{{la}}" longitude="{{lo}}" scale="14" markers="{{markers}}"></map><view class="address"><view style="color:#aaa;">选择收货地址</view><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['replace',['$0'],[[['addressList','',index]]]]]]]}}" class="detail" bindtap="__e"><view class="tag">{{''+item.addressTag+''}}</view><view style="margin-left:20rpx;width:460rpx;"><view>{{''+item.receiverCell+''}}</view><view style="color:#aaa;"><text>{{item.receiverName+item.receiverSex}}</text><text>{{item.receiverTel}}</text></view></view><view data-event-opts="{{[['tap',[['editAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="word" catchtap="__e"><text class="cuIcon-edit"></text></view></view></block></view><view data-event-opts="{{[['tap',[['goAddress',['$event']]]]]}}" class="add" bindtap="__e">新增收货地址</view></view>