page {
  height: 100%;
  background-color: #fff;
}
page .inputItem {
  width: 100%;
  height: 100rpx;
  padding: 20rpx 40rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
}
page .inputItem .inputBlock {
  width: calc(100% - 160rpx);
  height: 60rpx;
  background-color: #F6F6F6;
  border-radius: 50rpx;
  display: flex;
  padding: 0 20rpx;
  font-size: 36rpx;
  line-height: 60rpx;
  color: #b0b0b0;
  position: relative;
}
page .inputItem .inputBlock .inputFrame {
  margin-left: 10rpx;
  width: 80%;
  height: 60rpx;
  font-size: 26rpx;
  color: #626262;
}
page .inputItem .inputBlock .closeIcon {
  color: #cfcfcf;
  font-size: 30rpx;
  position: absolute;
  right: 20rpx;
}
page .inputItem .searchButton {
  width: 130rpx;
  height: 60rpx;
  border-radius: 50rpx;
  background-color: #008CFF;
  color: #fff;
  font-size: 28rpx;
  line-height: 60rpx;
  margin: 0;
}
page .historyItem {
  width: 100%;
  height: calc(100% - 100rpx);
  background-color: #fff;
  padding: 0 40rpx;
}
page .historyItem .historyTop {
  display: flex;
  justify-content: space-between;
}
page .historyItem .historyTop .cuIcon-titles {
  line-height: 70rpx;
  color: #008CFF;
}
page .historyItem .historyTop .historyTitle {
  font-size: 28rpx;
  color: #000000;
  line-height: 70rpx;
  margin-right: 480rpx;
}
page .historyItem .historyTop .deleteIcon {
  font-size: 36rpx;
  color: #b0b0b0;
  line-height: 70rpx;
}
page .historyItem .historyTops {
  display: flex;
}
page .historyItem .historyTops .cuIcon-titles {
  line-height: 70rpx;
  color: #008CFF;
}
page .historyItem .historyTops .historyTitle {
  font-size: 28rpx;
  color: #000000;
  line-height: 70rpx;
  margin-right: 480rpx;
}
page .historyItem .historyTops .deleteIcon {
  font-size: 36rpx;
  color: #b0b0b0;
  line-height: 70rpx;
}
page .historyItem .historyBlock {
  display: flex;
  flex-wrap: wrap;
  margin-top: 16rpx;
}
page .historyItem .historyBlock .historyText {
  height: 56rpx;
  border-radius: 50rpx;
  padding: 0 20rpx;
  line-height: 56rpx;
  background-color: #F6F6F6;
  color: #656565;
  font-size: 26rpx;
  margin: 0 30rpx 30rpx 0;
  white-space: nowrap;
  overflow: hidden;
}
page .noSearch {
  position: absolute;
  width: 100%;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  text-align: center;
}
page .noSearch image {
  width: 300rpx;
}
page .goodItem {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 20rpx;
  background-color: #f1f1f1;
}
page .goodItem .goodBlock {
  width: 340rpx;
  height: 480rpx;
  background-color: #fff;
  border-radius: 15rpx;
  padding: 20rpx 20rpx;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  margin-bottom: 20rpx;
}
page .goodItem .goodBlock .goodImg {
  width: 300rpx;
  height: 300rpx;
  border-radius: 15rpx;
}
page .goodItem .goodBlock .goodInfo {
  width: 100%;
  height: 35%;
}
page .goodItem .goodBlock .goodInfo .goodName {
  font-size: 30rpx;
  line-height: 40rpx;
  color: #101010;
  padding: 20rpx 0;
}
page .goodItem .goodBlock .goodInfo .goodPrice {
  font-size: 28rpx;
  line-height: 40rpx;
  height: 40rpx;
  color: #F86500;
  display: flex;
  justify-content: space-between;
  /* .money{
							font-size: 26rpx;
						}
						.originalPrice2 {
							color: #BCBCBC;
							text-decoration: line-through;
							font-size: 24rpx;
							margin-right: 10rpx;
						} */
}
page .goodItem .goodBlock .goodInfo .goodPrice image {
  width: 40rpx;
  height: 40rpx;
}
page .goodItem .goodBlock .goodInfo .goodPrice .goodBox {
  width: 100rpx;
}
page .goodItem .goodBlock .goodInfo .goodPrice .goodssugges {
  font-size: 36rpx;
}
page .goodItem .goodBlock .goodInfo .goodPrice .goodsize {
  font-size: 12px;
  color: #bbb;
  text-decoration: line-through;
  margin-top: -20rpx;
}
page .goodItem .goodBlock .goodInfo .goodPrice .registerButton {
  border-radius: 10rpx;
  color: #fff;
  background-color: #f5542a;
  text-align: center;
  height: 50rpx;
  width: 130rpx;
  line-height: 50rpx;
  margin-left: 70rpx;
  margin-top: 10rpx;
  font-size: 13px;
}
.menu {
  display: flex;
  background-color: #fff;
  justify-content: space-around;
}
.titleActive {
  color: #008CFF;
}

