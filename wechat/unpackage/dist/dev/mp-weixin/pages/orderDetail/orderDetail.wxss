.container {
  width: 100%;
}
.container .Completed {
  width: 100%;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
  margin: 20rpx;
}
.container .Completed .contact {
  display: flex;
  margin-top: 20rpx;
}
.container .Completed .contact .customer {
  width: 172rpx;
  height: 54rpx;
  color: #999999;
  background-color: #ffffff;
  border: 1px solid #cccccc;
  text-align: center;
  line-height: 52rpx;
  border-radius: 30rpx;
}
.container .Completed .contact .another {
  width: 172rpx;
  height: 54rpx;
  color: #ffffff;
  background-color: #fe771e;
  text-align: center;
  line-height: 52rpx;
  border-radius: 30rpx;
  margin-left: 50rpx;
}
.container .urbanItem {
  background-color: #ffffff;
  padding: 20rpx;
  margin: 20rpx;
}
.container .urbanItem .urbanMove {
  display: flex;
  align-items: center;
}
.container .urbanItem .urbanMove .cityArea {
  margin-left: 30rpx;
}
.container .informationItem {
  background-color: #ffffff;
  padding: 20rpx;
  margin: 20rpx;
}
.container .informationItem .orderNews {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .informationItem .registOrder {
  margin-bottom: 30rpx;
  border-bottom: 1px solid #e6e6e6;
  padding-bottom: 20rpx;
}
.container .informationItem .registOrder .Ordervive {
  display: flex;
  align-items: center;
}
.container .informationItem .registOrder .Ordervive .trademarking {
  width: 70%;
  margin-left: 20rpx;
}
.container .informationItem .totalPrice {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
  border-bottom: 1px solid #e6e6e6;
  padding-bottom: 20rpx;
}
.container .remarkItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #e6e6e6;
  background-color: #ffffff;
  padding: 20rpx;
  margin: 20rpx;
}
.container .messageItem {
  background-color: #ffffff;
  padding: 20rpx;
  margin: 20rpx;
}
.container .messageItem .copy {
  color: #0a90fe;
  border: 1px solid #0a90fe;
  width: 50rpx;
  height: 20rpx;
  padding: 1rpx 10rpx;
  border-radius: 6rpx;
  margin-left: 20rpx;
}

